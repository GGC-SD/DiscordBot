const mongoose = require("mongoose");


const roleTimeSchema = new mongoose.Schema({
    roleID: {
      type: String,
      required: true,
    },
    roleNames: {
      type: String,
      required: true,
    },
    days: {
      type: int,
      required: true,
    },
  });

  const RoleTimeDB = mongoose.model("RoleTime", roleTimeSchema);

  module.exports = {
    RoleTimeDB
  }