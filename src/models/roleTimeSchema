const mongoose = require("mongoose");


const roleTimeSchema = new mongoose.Schema({
    userId: {
            type: String,
            required: true,
        },
    userName: {
        type: String,
        required: true,
    },
    roleID: {
      type: String,
      required: true,
    },
    days: {
      type: int,
      required: true,
    },
  });

  const RoleTimeDB = mongoose.model("RoleTime", roleTimeSchema);

  module.exports = {
    RoleTimeDB
  }