const { SnowflakeUtil, User } = require("discord.js");
const mongoose = require("mongoose");


const inactivitySchema = new mongoose.Schema({
  userId: {
    type: String,
    required: true,
    unique: true,
  },
  lastMessageDate: {
    type: Date,
  },
  // userId: { type: SnowflakeUtil, require: true, unique: true },
  // userName: {type: String, require: true},
  // guildId: { type: SnowflakeUtil, require: true },
  // rolename requirement false because user can be added from role or inactivtyList 
  // roleName: {type: SnowflakeUtil, require: false},
  
});

const inactiveDB = mongoose.model("InactiveUser", inactivitySchema);
// await inactiveDB.create([]);
module.exports = {
  inactiveDB
}