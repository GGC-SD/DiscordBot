const mongoose = require("mongoose");

const inactivitySchema = new mongoose.Schema({
  userId: {
    type: String,
    required: true
  },
  userName: {
    type: String,
    required: true,
  },
  lastMessageDate: {
    type: Date,
    required: true
  },
});


const Inactivity = mongoose.model("InactiveUser", inactivitySchema);

module.exports = Inactivity;
